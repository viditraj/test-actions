{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "src",
  "projects": {
    "uds-common-MFE-template": {
      "root": "src/uds-common-MFE-template",
      "sourceRoot": "src/uds-common-MFE-template",
      "projectType": "library",
      "prefix": "uds-common-MFE-template",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "src/uds-common-MFE-template/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "src/uds-common-MFE-template/tsconfig.lib.json"
            },
            "development": {
              "tsConfig": "src/uds-common-MFE-template/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:web-test-runner",
          "options": {
            "main": "src/uds-common-MFE-template/src/jasmine-setup.ts",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "src/uds-common-MFE-template/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "styles": [
              "src/uds-common-MFE-template/src/styles.scss"
            ],
            "scripts": [],
            "assets": [
              "src/uds-common-MFE-template/src/favicon.ico",
              "src/uds-common-MFE-template/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "src/uds-common-MFE-template/**/*.ts",
              "src/uds-common-MFE-template/**/*.html"
            ]
          }
        }
      }
    },
